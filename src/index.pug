html
  head
    title Spec
    meta(charset='UTF-8')
    meta(http-equiv='Content-Security-Policy',
      content=`default-src 'none'; img-src vscode-resource: https:; script-src 'nonce-${nonce}'; style-src 'nonce-${nonce}' https:; font-src data: vscode-resource: https:; worker-src blob:; connect-src vscode-resource: ws:`)
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    script(nonce=`${nonce}`).
      rootPath = '#{song_path}'
    script(nonce=`${nonce}`, src=`${jquery_uri}`)
    script(nonce=`${nonce}`, src=`${semjs_uri}`)
    link(nonce=`${nonce}`, rel='stylesheet', type="text/css", href=`${semcss_uri}`)
    link(nonce=`${nonce}`, rel='stylesheet', type='text/css', href=`${ctmcss_uri}`)
  body.vsc_bg.overflow
    canvas#canvas
    script(nonce=`${nonce}`, src=`${bundle_uri}`)