{
  "name": "spectrogram",
  "displayName": "Spectrogram",
  "description": "Real-time spectrogram",
  "version": "2.0.0",
  "publisher": "lanly-dev",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Audio",
    "spectrogram",
    "mp3",
    "flac"
  ],
  "icon": "media/spec.png",
  "galleryBanner": {
    "color": "white",
    "theme": "light"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "spectrogram-explorer",
          "name": "spectrogram",
          "icon": "media/icon.svg",
          "contextualTitle": "Spectrogram"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "vsce-package": "vsce package",
    "package": "webpack --mode production --devtool hidden-source-map",
    "wp": "webpack",
    "wp-watch": "webpack --watch",
    "task-clean-output": "ts-node tasks.js clean",
    "lint": "eslint --format summary-chart .",
    "lint-fix": "eslint --fix ."
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/vscode": "^1.96.0",
    "@types/vscode-webview": "^1.57.5",
    "@vscode/codicons": "^0.0.36",
    "@vscode/vsce": "^3.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.19.0",
    "eslint-formatter-summary-chart": "^0.3.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "pug": "^3.0.3",
    "shelljs": "^0.8.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lanly-dev/VSCode-Spectrogram.git"
  }
}
